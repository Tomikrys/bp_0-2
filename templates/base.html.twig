                </div>
            </div>
        </div>
    </div>
</main>
{{ include('parts/footer.html.twig') }}
<script src="{{ asset('js/script.js?ver=1.0') }}"></script>
<script src="{{ asset('js/dragula.min.js') }}"></script>
<script src="{{ asset('js/sorttable.js') }}"></script>
<script>
    sorttable.sort_alpha = function(a,b) { return a[0].localeCompare(b[0]); }
    if(navigator.userAgent.toLowerCase().indexOf('chrome') === -1){
        document.getElementById("cite").classList.remove("text-muted");
        document.getElementById("cite").classList.add("text-danger");
        document.getElementById("cite").innerText="Při problémech použijte prohlížeč Chrome!"
    }
</script>
{% block javascripts %}{% endblock %}
</body>
